<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Wyswietl" android:paddingBottom="10dp">
    <ImageView
            android:layout_width="146dp"
            android:layout_height="147dp" app:srcCompat="@android:drawable/btn_dialog"
            android:id="@+id/imageView8" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="Nazwa LA"
            android:layout_width="88dp"
            android:layout_height="32dp"
            android:id="@+id/nazwa_la"
            android:paddingBottom="5dp"
            android:textAlignment="center" android:paddingTop="5dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/nazwa_pl" app:layout_constraintStart_toEndOf="@+id/imageView8"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="Nazwa PL"
            android:layout_width="88dp"
            android:layout_height="32dp"
            android:id="@+id/nazwa_pl" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/imageView8"
            android:layout_marginStart="8dp" android:textAlignment="center"
            android:paddingBottom="5dp" android:paddingTop="5dp"/>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="147dp"
            android:layout_height="103dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView8" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp">
        <CheckBox
                android:text="Pomnik przyrody"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/pomnik_przyrody"/>
        <CheckBox
                android:text="Szklarnia"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/szklarnia"/>
        <CheckBox
                android:text="Nie istnieje"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/nie_istnieje"/>
    </LinearLayout>
    <TextView
            android:text="@string/none"
            android:layout_width="138dp"
            android:layout_height="31dp"
            android:id="@+id/textView5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:hint="Nazwa Polska"
            app:layout_constraintStart_toEndOf="@+id/nazwa_pl" android:layout_marginStart="8dp"
            android:textAlignment="center" android:paddingTop="5dp"/>
    <TextView
            android:text="@string/none"
            android:layout_width="138dp"
            android:layout_height="33dp"
            android:id="@+id/textView9"
            android:hint="Nazwa Polska"
            android:textAlignment="center" android:paddingTop="5dp"
            app:layout_constraintStart_toEndOf="@+id/nazwa_la"
            android:layout_marginStart="8dp" app:layout_constraintTop_toBottomOf="@+id/textView5"
            android:layout_marginTop="8dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Adres"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10" android:paddingBottom="5dp"
            android:paddingLeft="10dp" android:paddingTop="5dp" android:paddingRight="10dp"
            app:layout_constraintTop_toBottomOf="@+id/nazwa_la"
            app:layout_constraintStart_toEndOf="@+id/imageView8" android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="8dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="198dp"
            android:layout_height="28dp"
            android:id="@+id/textView11" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10" app:layout_constraintStart_toEndOf="@+id/imageView8"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
</android.support.constraint.ConstraintLayout>