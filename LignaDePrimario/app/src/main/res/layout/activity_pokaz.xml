<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Pokaz">
    <TextView
            android:text="@string/none"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/lokalizacja_drzewa_db" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/nazwa_drzewa_latina_db" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="68dp" android:textAlignment="center" android:padding="5dp"/>
    <TextView
            android:text="Lokalizacja"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:id="@+id/lokacja_drzewa_View" android:padding="5dp"
            app:layout_constraintTop_toBottomOf="@+id/nazwa_drzewa_Latina_View"
            android:textAlignment="center"
            android:layout_marginTop="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/lokalizacja_drzewa_db" app:layout_constraintHorizontal_bias="1.0"/>
    <WebView
            android:layout_width="232dp"
            android:layout_height="167dp"
            android:id="@+id/webView" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.564"/>
    <TextView
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/nazwa_drzewa_pl_db" android:text="@string/none"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/webView"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="68dp" android:textAlignment="center"
            android:padding="5dp"/>
    <TextView
            android:text="Nazwa PL"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:id="@+id/nazwa_drzewa_PLView" android:padding="5dp"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/webView"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/nazwa_drzewa_pl_db"
            app:layout_constraintHorizontal_bias="1.0" android:textAlignment="center"/>
    <TextView
            android:text="@string/none"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/nazwa_drzewa_latina_db"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/nazwa_drzewa_pl_db"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="68dp" android:textAlignment="center"
            android:padding="5dp"/>
    <TextView
            android:text="Nazwa L"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:id="@+id/nazwa_drzewa_Latina_View" android:padding="5dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/nazwa_drzewa_PLView"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/nazwa_drzewa_latina_db"
            app:layout_constraintHorizontal_bias="1.0" android:textAlignment="center"/>
    <TextView
            android:text="@string/none"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/type_drzewa_db"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="68dp" android:textAlignment="center" android:padding="5dp"
            app:layout_constraintTop_toBottomOf="@+id/lokalizacja_drzewa_db"
            android:layout_marginTop="8dp"/>
    <TextView
            android:text="Rodzaj"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:id="@+id/type_drzewa_View" android:padding="5dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/lokacja_drzewa_View"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/type_drzewa_db"
            app:layout_constraintHorizontal_bias="1.0" android:textAlignment="center"/>
</android.support.constraint.ConstraintLayout>